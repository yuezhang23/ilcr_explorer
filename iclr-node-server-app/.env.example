# Frontend URL
FRONTEND_URL=http://localhost:3000

# Node Environment
NODE_ENV=development

# Session Secret (replace with a strong secret)
SESSION_SECRET=your_session_secret_here

# Database Connection String (replace with your MongoDB connection string)
DB_CONNECTION_STRING=mongodb+srv://username:password@cluster.mongodb.net/database?retryWrites=true&w=majority

# OpenAI API Key (replace with your actual API key)
OPENAI_API_KEY=your_openai_api_key_here
