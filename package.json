{
  "name": "iclr-rating-dev",
  "version": "1.0.0",
  "description": "ICLR Rating Development Project",
  "scripts": {
    "install-all": "npm install && cd iclr-node-server-app && npm install && cd ../iclr-react-web-app && npm install",
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:backend": "cd iclr-node-server-app && npm start",
    "start:frontend": "cd iclr-react-web-app && npm start",
    "build": "cd iclr-react-web-app && npm run build",
    "test": "cd iclr-react-web-app && npm test"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": ["iclr", "rating", "fullstack"],
  "author": "",
  "license": "ISC"
} 